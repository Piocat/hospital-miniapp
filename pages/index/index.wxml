<ask wx:if="{{PageCur=='ask'}}"></ask>
<hospital wx:if="{{PageCur=='hospital'}}"></hospital>
<notice id="notice" wx:if="{{PageCur=='notice'}}" cur="notice"></notice>
<my wx:if="{{PageCur=='my'}}"></my>
<view class="cu-bar tabbar bg-white shadow foot">
  <view class="action" bindtap="NavChange" data-cur="ask">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/basics{{PageCur=='ask'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='ask'?'text-green':'text-gray'}}">主页</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="hospital">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/component{{PageCur=='hospital'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='hospital'?'text-green':'text-gray'}}">简介</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="notice">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/plugin{{PageCur=='notice'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='notice'?'text-green':'text-gray'}}">通知</view>
  </view>
  <view class="action" bindtap="NavChange" data-cur="my">
    <view class='cuIcon-cu-image'>
      <image src="/images/tabbar/about{{PageCur=='my'?'_cur':''}}.png"></image>
    </view>
    <view class="{{PageCur=='my'?'text-green':'text-gray'}}">我的</view>
  </view>
</view>

